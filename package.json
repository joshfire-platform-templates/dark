{
  "name": "dark",
  "version": "1.0",
  "fullName": "Dark",
  "description": "A template with a dark color scheme. It has the same features than the \"Base\" template. It can be used for any purpose.",
  "keywords": [
     "base",
     "dark",
     "default",
     "black"
   ],
  "joshfire": {
    "images": {
      "small":"marketing/image-small.png",
      "big":"marketing/image-big.png"
    },
    "devices": [
      {
        "devices": "phone",
        "startfile": "app/index.phone.html",
        "images":"marketing/preview-phone.png",
        "screenshots":["marketing/screenshot-phone-1.png", "marketing/screenshot-phone-2.png", "marketing/screenshot-phone-3.png"]
      },
      {
        "devices": "tablet",
        "startfile": "app/index.tablet.html",
        "images":"marketing/preview-tablet.png",
        "screenshots":["marketing/screenshot-tablet-1.png", "marketing/screenshot-tablet-2.png"]  
      }
    ],
    "datasources": {
      "main": {
        "title": "Main tabs",
        "description": "Add up to 6 tabs in your application",
        "multiple":true
      }
    },
    "options": {
      "schema": {
        "abouthtml": {
          "type": "string"
        }
      },
    "form": [
      "datasources.main",
      {
        "key": "abouthtml",
        "type": "textarea"
      }
    ]
    }
  }
}